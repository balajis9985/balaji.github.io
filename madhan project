        "# [Verified] Display the best job for each candidate from LinkedIn along with the similarity score\n",
        "print(\"Best LinkedIn Job Matches for Verified Candidates (Similarity > 0.15):\")\n",
        "for i, match in enumerate(best_matches_linkedin_ver):\n",
        "  if sim_linkedin_ver[i][match] > 0.15:\n",
        "    print(f\"Candidate {i}: Job Title - {linkedin_jobs.loc[match, 'title']} (Similarity: {sim_linkedin_ver[i][match]:.2f})\")\n",
        "\n",
        "# [Unverified] Display the best job for each candidate from LinkedIn along with the similarity score\n",
        "print(\"\\nBest LinkedIn Job Matches for Unverified Candidates (Similarity > 0.10):\")\n",
        "for i, match in enumerate(best_matches_linkedin_unver):\n",
        "  if sim_linkedin_unver[i][match] > 0.10:\n",
        "    print(f\"Candidate {i}: Job Title - {linkedin_jobs.loc[match, 'title']} (Similarity: {sim_linkedin_unver[i][match]:.2f})\")\n",
        "\n",
        "# [Verified] Display the best job for each candidate from GetOnBoard along with the similarity score\n",
        "print(\"\\nBest GetOnBoard Job Matches for Verified Candidates (Similarity > 0.10):\")\n",
        "for i, match in enumerate(best_matches_geton_ver):\n",
        "  if sim_geton_ver[i][match] > 0.10:\n",
        "    print(f\"Candidate {i}: Job Title - {geton_jobs.loc[match, 'Title']} (Similarity: {sim_geton_ver[i][match]:.2f})\")\n",
        "\n",
        "# [Unverified] Display the best job for each candidate from GetOnBoard along with the similarity score\n",
        "print(\"\\nBest GetOnBoard Job Matches for Unverified Candidates (Similarity > 0.10):\")\n",
        "for i, match in enumerate(best_matches_geton_unver):\n",
        "  if sim_geton_unver[i][match] > 0.10:\n",
        "    print(f\"Candidate {i}: Job Title - {geton_jobs.loc[match, 'Title']} (Similarity: {sim_geton_unver[i][match]:.2f})\")\n",
        "\n",
        "# Compare best matches for verified vs. unverified candidates for LinkedIn\n",
        "differences_li = best_matches_linkedin_ver - best_matches_linkedin_unver\n",
        "print(\"\\nDifferences in best match indices for LinkedIn (Verified vs. Unverified):\")\n",
        "print([x for x in differences_li if x != 0])\n",
        "\n",
        "# Compare best matches for verified vs. unverified candidates for GetOnBoard\n",
        "differences_geton = best_matches_geton_ver - best_matches_geton_unver\n",
        "print(\"Differences in best match indices for GetOnBoard (Verified vs. Unverified):\")\n",
        "print([x for x in differences_geton if x != 0])\n",
        "\n",
        "# Optionally, compare candidate skills and job title match for verified LinkedIn matches\n",
