      "cell_type": "code",
      "source": [
        "#===================TF-IDF Embedding====================="
      ],
      "metadata": {
        "id": "yZJYv-J_qvhR"
      },
      "execution_count": 139,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# Reload df_verified and df_unverified from the correct CSV files\n",
        "df_verified = pd.read_csv('VerifiedDataset.csv', on_bad_lines='skip')\n",
        "df_unverified = pd.read_csv('stackoverflow_full.csv', on_bad_lines='skip')\n",
        "\n",
        "#TFIDF Encoding on verified dataset\n",
        "vectorizer = TfidfVectorizer()\n",
        "\n",
        "all_text = pd.concat([df_verified[\"HaveWorkedWith\"],df_unverified[\"HaveWorkedWith\"], linkedin_jobs[\"description\"], geton_jobs[\"Description\"]]).dropna()\n",
        "vectorizer.fit(all_text)\n",
        "\n",
        "# Transform each dataset separately using the same vectorizer\n",
        "verified_tfidf = vectorizer.transform(df_verified[\"HaveWorkedWith\"])\n",
        "unverified_tfidf = vectorizer.transform(df_unverified[\"HaveWorkedWith\"])\n",
        "\n",
        "linkedin_jobs_tfidf = vectorizer.transform(linkedin_jobs[\"description\"])\n",
        "geton_jobs_tfidf = vectorizer.transform(geton_jobs[\"Description\"])"
      ],
      "metadata": {
        "id": "DQ5T6pgiWSs_"
      },
      "execution_count": 140,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "import pandas as pd\n",
        "from sklearn.feature_extraction.text import TfidfVectorizer\n",
        "\n",
        "# -----------------------------\n",
        "# 1. Load datasets from Drive\n",
        "# -----------------------------\n",
        "df_verified = pd.read_csv(\n",
        "    \"https://drive.google.com/uc?export=download&id=1ENB8BD8j0XgYPFMhbGCu2CJaZ5dziTRp\",\n",
        "    on_bad_lines='skip'\n",
        ")\n",
        "\n",
        "df_unverified = pd.read_csv(\n",
        "    \"https://drive.google.com/uc?export=download&id=1l99VNNum1Kncs63R96HraZ2xAf_mwyye\",\n",
        "    on_bad_lines='skip'\n",
        ")\n",
        "\n",
        "linkedin_jobs = pd.read_csv(\n",
        "    \"https://drive.google.com/uc?export=download&id=1TS3WsZUH3V667RG4tKm_eigbJAfzNoqO\",\n",
        "    on_bad_lines='skip'\n",
        ")\n",
        "\n",
        "geton_jobs = pd.read_csv(\n",
        "    \"https://drive.google.com/uc?export=download&id=1uHXcN8vjh7hUGsVI5eHKCu6lki8sVNF_\",\n",
        "    on_bad_lines='skip'\n",
        ")\n",
        "\n",
        "sim_linkedin = pd.read_csv(\n",
        "    \"https://drive.google.com/uc?export=download&id=1WDLrpqYt2OYgRzhiK0Vw67T_I-nQCR6G\",\n",
        "    on_bad_lines='skip'\n",
        ")\n",
        "\n",
        "# -----------------------------\n",
        "# 2. Inspect column names\n",
        "# -----------------------------\n",
        "print(\"Verified columns:\", df_verified.columns.tolist())\n",
        "print(\"Unverified columns:\", df_unverified.columns.tolist())\n",
        "\n",
        "# -----------------------------\n",
        "# 3. Standardize column names to lowercase\n",
        "# -----------------------------\n",
        "df_verified.columns = df_verified.columns.str.strip().str.lower()\n",
        "df_unverified.columns = df_unverified.columns.str.strip().str.lower()\n",
        "linkedin_jobs.columns = linkedin_jobs.columns.str.strip().str.lower()\n",
        "geton_jobs.columns = geton_jobs.columns.str.strip().str.lower()\n",
        "\n",
        "# The text column might have a different name; try to map it\n",
        "text_col_verified = \"haveworkedwith\" if \"haveworkedwith\" in df_verified.columns else df_verified.columns[0]\n",
        "text_col_unverified = \"haveworkedwith\" if \"haveworkedwith\" in df_unverified.columns else df_unverified.columns[0]\n",
        "\n",
        "# -----------------------------\n",
        "# 4. Drop rows with missing text\n",
        "# -----------------------------\n",
        "df_verified = df_verified.dropna(subset=[text_col_verified])\n",
        "df_unverified = df_unverified.dropna(subset=[text_col_unverified])\n",
        "\n",
        "# Ensure job descriptions exist\n",
        "if 'description' not in linkedin_jobs.columns:\n",
        "    linkedin_jobs['description'] = \"\"\n",
        "if 'description' not in geton_jobs.columns:\n",
        "    geton_jobs['description'] = \"\"\n",
        "\n",
        "# -----------------------------\n",
        "# 5. Parse text\n",
        "# -----------------------------\n",
        "def parse_text(text):\n",
        "    return str(text).replace(\"\\n\", \" \").strip()\n",
        "\n",
        "linkedin_jobs['description'] = linkedin_jobs['description'].apply(parse_text)\n",
        "geton_jobs['description'] = geton_jobs['description'].apply(parse_text)\n",
        "\n",
        "# -----------------------------\n",
        "# 6. TF-IDF Encoding\n",
        "# -----------------------------\n",
        "vectorizer = TfidfVectorizer()\n",
        "\n",
        "vectorizer.fit(\n",
        "    pd.concat([\n",
        "        df_verified[text_col_verified],\n",
        "        df_unverified[text_col_unverified],\n",
        "        linkedin_jobs['description'],\n",
        "        geton_jobs['description']\n",
        "    ])\n",
        ")\n",
        "\n",
        "verified_tfidf = vectorizer.transform(df_verified[text_col_verified])\n",
        "unverified_tfidf = vectorizer.transform(df_unverified[text_col_unverified])\n",
        "linkedin_jobs_tfidf = vectorizer.transform(linkedin_jobs['description'])\n",
        "geton_jobs_tfidf = vectorizer.transform(geton_jobs['description'])\n",
        "\n",
        "print(\"TF-IDF shapes:\")\n",
        "print(\"Verified:\", verified_tfidf.shape)\n",
        "print(\"Unverified:\", unverified_tfidf.shape)\n",
        "print(\"LinkedIn:\", linkedin_jobs_tfidf.shape)\n",
        "print(\"GetOn:\", geton_jobs_tfidf.shape)\n"
      ],
      "metadata": {
        "id": "eyjCg6wcTvGc",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "bc7c2778-68c0-46da-8468-520bcff1a807"
      },
      "execution_count": 141,
      "outputs": [
        {
          "output_type": "stream",
