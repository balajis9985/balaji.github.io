    {
      "cell_type": "code",
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 887
        },
        "id": "2c76d5bb",
        "outputId": "209778f0-c5c8-4afc-be14-71c3d41a9041"
      },
      "source": [
        "# Handle missing values in 'HaveWorkedWith'\n",
        "df_verified.dropna(subset=['HaveWorkedWith'], inplace=True)\n",
        "df_unverified.dropna(subset=['HaveWorkedWith'], inplace=True)\n",
        "\n",
        "# Handle missing values in job description columns by replacing NaN with empty strings\n",
        "linkedin_jobs['description'].fillna('', inplace=True)\n",
        "geton_jobs['Description'].fillna('', inplace=True)\n",
        "\n",
        "# Apply the parse_text_linkedin function to the 'HaveWorkedWith' column\n",
        "df_verified['HaveWorkedWith'] = df_verified['HaveWorkedWith'].apply(lambda x: parse_text_linkedin(str(x)))\n",
        "df_unverified['HaveWorkedWith'] = df_unverified['HaveWorkedWith'].apply(lambda x: parse_text_linkedin(str(x)))\n",
        "\n",
        "# Apply the parse_text_linkedin function to the 'description' column of linkedin_jobs\n",
        "linkedin_jobs['description'] = linkedin_jobs['description'].apply(lambda x: parse_text_linkedin(str(x)))\n",
        "\n",
        "# Apply the parse_text_getonjobs function to the 'Description' column of geton_jobs\n",
        "geton_jobs['Description'] = geton_jobs['Description'].apply(lambda x: parse_text_getonjobs(str(x)))\n",
        "\n",
        "# Display the first few rows of the cleaned columns\n",
        "print(\"Cleaned df_verified 'HaveWorkedWith':\")\n",
        "display(df_verified[['HaveWorkedWith']].head())\n",
        "\n",
        "print(\"\\nCleaned df_unverified 'HaveWorkedWith':\")\n",
        "display(df_unverified[['HaveWorkedWith']].head())\n",
        "\n",
        "print(\"\\nCleaned linkedin_jobs 'description':\")\n",
        "display(linkedin_jobs[['description']].head())\n",
        "\n",
        "print(\"\\nCleaned geton_jobs 'Description':\")\n",
