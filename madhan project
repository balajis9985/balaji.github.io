      "cell_type": "code",
      "source": [
        "col_name = \"HaveWorkedWith\"\n",
        "\n",
        "if col_name in df_verified.columns:\n",
        "    nan_count_verified = df_verified[col_name].isna().sum()\n",
        "    df_verified_clean = df_verified.dropna(subset=[col_name])\n",
        "else:\n",
        "    nan_count_verified = \"Column missing\"\n",
        "    df_verified_clean = df_verified.copy()\n",
        "\n",
        "if col_name in df_unverified.columns:\n",
        "    nan_count_unverified = df_unverified[col_name].isna().sum()\n",
        "    df_unverified_clean = df_unverified.dropna(subset=[col_name])\n",
        "else:\n",
        "    nan_count_unverified = \"Column missing\"\n",
        "    df_unverified_clean = df_unverified.copy()\n",
        "\n",
        "print(f\"NaN count for verified data: {nan_count_verified}\")\n",
        "print(f\"NaN count for unverified data: {nan_count_unverified}\")\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "id": "rioxHjjcTfmV",
        "outputId": "bf88bfb2-2ed9-45fd-852e-4b764d04e1db"
      },
      "execution_count": 126,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
          "text": [
            "NaN count for verified data: Column missing\n",
            "NaN count for unverified data: Column missing\n"
          ]
        }
