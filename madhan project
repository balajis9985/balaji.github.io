        "# Reload the linkedin_jobs dataframe from the correct CSV file to ensure it's using the right data\n",
        "linkedin_jobs = pd.read_csv('LinkedIn Jobs-Table 1.csv', on_bad_lines='skip')\n",
        "\n",
        "# Step 1: Check available columns in LinkedIn dataset\n",
        "print(\"LinkedIn Jobs Columns used in this cell:\", linkedin_jobs.columns.tolist())\n",
        "\n",
        "# Step 2: Use the correct description column name\n",
        "desc_col = 'description'\n",
        "\n",
        "if desc_col not in linkedin_jobs.columns:\n",
        "    raise ValueError(f\"Column '{desc_col}' not found in LinkedIn dataset.\")\n",
        "\n",
        "print(f\"Using description column: {desc_col}\")\n",
        "\n",
        "# Step 3: Apply the parse_text_linkedin function safely\n",
        "linkedin_jobs[desc_col] = linkedin_jobs[desc_col].apply(\n",
        "    lambda x: parse_text_linkedin(str(x)) if pd.notnull(x) else \"\"\n",
        ")\n",
        "\n",
        "# Step 4: Show updated DataFrame\n",
        "print(linkedin_jobs[['company_name', desc_col]].head())"
      ],
